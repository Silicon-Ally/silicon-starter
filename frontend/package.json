{
  "private": true,
  "scripts": {
    "typecheck": "nuxi typecheck",
    "lint": "eslint --cache --ext .js,.vue,.ts ./; stylelint --cache '**/*.{vue,css,scss}' ;",
    "lint:fix": "eslint --fix --ext .js,.vue,.ts ./; stylelint '**/*.{vue,css,scss}' --fix",
    "local": "DOTENV_PATH=envs/local.env nuxt dev",
    "frontend": "DOTENV_PATH=envs/local.env nuxt dev",
    "build:local": "LOCAL_PREVIEW_MODE=true DOTENV_PATH=envs/local.env nuxt build",
    "build:dev": "DOTENV_PATH=envs/dev.env nuxt build",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "graphql-gen": "graphql-codegen --config graphql/codegen.yml",
    "test": "jest --config=.jest/jest.config.ts",
    "test:coverage": "jest --config=.jest/jest.config.ts --collectCoverage=true",
    "deploy:dev": "NITRO_PRESET=firebase npm run build:dev && firebase deploy --project <dev project ID> --config firebase.dev.json"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-typescript": "^7.18.6",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@graphql-codegen/cli": "^2.15.0",
    "@graphql-codegen/typescript": "^2.8.2",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^2.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^6.6.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vue/vue3-jest": "^29.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.3.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.8.0",
    "firebase-functions": "^4.1.0",
    "firebase-functions-test": "^3.0.0",
    "firebase-tools": "^12.0.1",
    "jest": "^29.3.1",
    "nuxt": "^3.5.0",
    "postcss-html": "^1.5.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "stylelint": "^14.15.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vue-tsc": "^1.0.10"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "firebase": "^9.14.0",
    "firebase-admin": "^11.3.0",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "graphql-tag": "^2.12.6",
    "std-env": "^3.3.1",
    "vue": "^3.2.45"
  }
